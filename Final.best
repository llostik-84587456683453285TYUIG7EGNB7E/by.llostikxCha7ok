local function checkIfBeingAimed()
    if not Settings.Combat.AntiSniper.Enabled then return nil end
    local myChar = LocalPlayer.Character
    if not myChar then return nil end
    local myRoot = myChar:FindFirstChild("HumanoidRootPart")
    if not myRoot then return nil end
    local myPos = myRoot.Position

    for _, plr in ipairs(Players:GetPlayers()) do
        if plr == LocalPlayer then continue end
        local char = plr.Character
        if not char then continue end
        local head = char:FindFirstChild("Head")
        if not head then continue end
        local lookVector = head.CFrame.LookVector
        local dirToMe = (myPos - head.Position).Unit
        local dot = lookVector:Dot(dirToMe)
        if dot < Settings.Combat.AntiSniper.Sensitivity then continue end
        local dist = (head.Position - myPos).Magnitude
        if dist > Settings.Combat.AntiSniper.Range then continue end

        local raycastParams = RaycastParams.new()
        raycastParams.FilterType = Enum.RaycastFilterType.Blacklist
        raycastParams.FilterDescendantsInstances = {char, myChar, Camera}
        local result = Workspace:Raycast(head.Position, dirToMe * dist, raycastParams)
        if not result then
            return plr
        end
    end
    return nil
end

local tracerPool = {}
local function getTracerLine()
    for _, line in ipairs(tracerPool) do
        if not line.Visible then
            line.Visible = true
            return line
        end
    end
    local line = Drawing.new("Line")
    line.Thickness = Settings.Combat.BulletTracers.Thickness
    line.Color = Settings.Combat.BulletTracers.Color
    table.insert(tracerPool, line)
    return line
end

local function addTracer(startPos, endPos)
    if not Settings.Combat.BulletTracers.Enabled then return end
    task.spawn(function()
        local line = getTracerLine()
        local fromScreen = Camera:WorldToViewportPoint(startPos)
        local toScreen = Camera:WorldToViewportPoint(endPos)
        line.From = Vector2.new(fromScreen.X, fromScreen.Y)
        line.To = Vector2.new(toScreen.X, toScreen.Y)
        line.Visible = true
        task.wait(Settings.Combat.BulletTracers.Duration)
        line.Visible = false
    end)
end

