    local tool = char:FindFirstChildOfClass("Tool")
    if not tool then return end

    local attrsToZero = {"Recoil", "RecoilStrength", "Spread", "kick", "shake"}
    for _, attr in ipairs(attrsToZero) do
        pcall(function()
