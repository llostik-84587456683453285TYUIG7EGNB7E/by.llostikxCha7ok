    label.Text = displayName
    label.TextColor3 = COLORS.WHITE
    label.Font = Enum.Font.Gotham
    label.TextSize = 16
    label.TextXAlignment = Enum.TextXAlignment.Left

    if desc then
        local d = Instance.new("TextLabel")
        d.Parent = btnFrame
        d.BackgroundTransparency = 1
