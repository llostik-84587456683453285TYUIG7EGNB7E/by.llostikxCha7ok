local currentTab = "Aimbot"

local function setTabCorner(btn, radius)
    local corner = Instance.new("UICorner")
    corner.CornerRadius = UDim.new(0, radius)
    for _, child in ipairs(btn:GetChildren()) do
        if child:IsA("UICorner") then
            child:Destroy()
        end
    end
    corner.Parent = btn
end

for i, tabName in ipairs(tabs) do
    local btn = Instance.new("TextButton")
    btn.Parent = tabContainer
    btn.Size = UDim2.new(0, math.floor(1050/#tabs), 1, -5)
    btn.Position = UDim2.new(0, (i-1)*math.floor(1050/#tabs), 0, 2)
    btn.BackgroundColor3 = (tabName == currentTab) and COLORS.PINK_HOT or COLORS.GRAY_MED
    btn.BackgroundTransparency = 0.3
    btn.Text = tabName
    btn.TextColor3 = COLORS.WHITE
    btn.Font = Enum.Font.GothamBold
    btn.TextSize = 16
    btn.BorderSizePixel = 0
    btn.ClipsDescendants = true

    if tabName == currentTab then
        setTabCorner(btn, 10)
    end

    btn.MouseButton1Click:Connect(function()
        currentTab = tabName
        for _, b in ipairs(tabButtons) do
            b.BackgroundColor3 = COLORS.GRAY_MED
            local tween = TweenService:Create(b, TweenInfo.new(0.2), {BackgroundColor3 = COLORS.GRAY_MED})
            tween:Play()
            setTabCorner(b, 0)
        end
        btn.BackgroundColor3 = COLORS.PINK_HOT
        local tween = TweenService:Create(btn, TweenInfo.new(0.2), {BackgroundColor3 = COLORS.PINK_HOT})
        tween:Play()
        setTabCorner(btn, 10)
        updateTabContent(tabName)
    end)
    table.insert(tabButtons, btn)
end

