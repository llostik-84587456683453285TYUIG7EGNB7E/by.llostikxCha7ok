if isfolder and writefile and readfile then
    local function SaveConfig(profile)
        profile = profile or Settings.ConfigProfile
        local saveTable = {
            ESP = Settings.ESP,
            Aimbot = Settings.Aimbot,
            BotsAim = Settings.BotsAim,
            Misc = Settings.Misc,
            DamageIndicator = Settings.DamageIndicator,
            Combat = Settings.Combat,
            HvH = Settings.HvH,
            FakeLag = Settings.FakeLag,
            Triggerbot = Settings.Triggerbot,
            AdminESP = Settings.AdminESP,
            Visuals = Settings.Visuals,
            BulletPhysics = Settings.BulletPhysics,
        }
        pcall(function()
            writefile("GGshopXenoConfig_Profile" .. profile .. ".json", HttpService:JSONEncode(saveTable))
            game:GetService("StarterGui"):SetCore("SendNotification", {
                Title = "GGshop XENO",
                Text = "Profile " .. profile .. " saved!",
                Duration = 1
            })
        end)
    end

    local function LoadConfig(profile)
        profile = profile or Settings.ConfigProfile
        pcall(function()
            local data = HttpService:JSONDecode(readfile("GGshopXenoConfig_Profile" .. profile .. ".json"))
            if data then
                for k, v in pairs(data) do
                    if type(v) == "table" then
                        for sk, sv in pairs(v) do
                            Settings[k][sk] = sv
                        end
                    else
                        Settings[k] = v
                    end
                end
                game:GetService("StarterGui"):SetCore("SendNotification", {
                    Title = "GGshop XENO",
                    Text = "Profile " .. profile .. " loaded!",
                    Duration = 1
                })
            end
        end)
    end

    local lastProfileFile = "GGshopXeno_lastProfile.txt"
    pcall(function()
        if isfile(lastProfileFile) then
            Settings.ConfigProfile = tonumber(readfile(lastProfileFile)) or 1
        end
    end)

    LoadConfig()
end

