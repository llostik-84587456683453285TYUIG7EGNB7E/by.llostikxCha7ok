local function GetTargetUnderCrosshair()
    local params = RaycastParams.new()
    params.FilterType = Enum.RaycastFilterType.Blacklist
    params.FilterDescendantsInstances = {LocalPlayer.Character, Camera}

    local ray = Camera:ViewportPointToRay(Camera.ViewportSize.X/2, Camera.ViewportSize.Y/2)
    local result = Workspace:Raycast(ray.Origin, ray.Direction * Settings.Triggerbot.Range, params)

    if result and result.Instance then
        local hitPart = result.Instance
        local character = hitPart.Parent
        local player = Players:GetPlayerFromCharacter(character)

        if player and player ~= LocalPlayer then
            if Settings.Triggerbot.TeamCheck and player.Team == LocalPlayer.Team then
                return nil
            end

            local humanoid = character:FindFirstChildOfClass("Humanoid")
            if not humanoid or humanoid.Health <= 0 then
                return nil
            end

            if Settings.Triggerbot.WallCheck then
                local head = character:FindFirstChild("Head")
                if head and not IsTargetVisible(head, character) then
                    return nil
                end
            end

            if Settings.Triggerbot.Hitbox ~= "Any" then
                if hitPart.Name ~= Settings.Triggerbot.Hitbox and not hitPart.Name:find(Settings.Triggerbot.Hitbox) then
                    return nil
                end
            end

            return player, hitPart
        end
    end
    return nil
end

local lastShotTime = 0

local function Shoot()
    local now = tick()
    if now - lastShotTime < Settings.Triggerbot.Delay then
        return
    end

    VirtualInputManager:SendMouseButtonEvent(
        Camera.ViewportSize.X/2,
        Camera.ViewportSize.Y/2,
        0, true, game, 1
    )
    VirtualInputManager:SendMouseButtonEvent(
        Camera.ViewportSize.X/2,
        Camera.ViewportSize.Y/2,
        0, false, game, 1
    )

    lastShotTime = now
end

