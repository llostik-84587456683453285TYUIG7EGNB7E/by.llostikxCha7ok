local hitmarkerPool = {}
local function getHitmarkerText()
    for _, text in ipairs(hitmarkerPool) do
        if not text.Visible then
            text.Visible = true
            return text
        end
    end
    local text = Drawing.new("Text")
    text.Font = 2
    text.Outline = true
    text.Size = 14
    text.Color = COLORS.WHITE
    table.insert(hitmarkerPool, text)
    return text
end

local function addHitmarker(text, isHeadshot, damage)
    task.spawn(function()
        local marker = getHitmarkerText()
        marker.Text = text
        marker.Position = Vector2.new(50, Camera.ViewportSize.Y / 2)

        local yOffset = 0
        for _, m in ipairs(hitmarkerPool) do
            if m.Visible and m ~= marker then
                yOffset = yOffset + 20
            end
        end
        marker.Position = Vector2.new(50, Camera.ViewportSize.Y / 2 + yOffset)

        if (isHeadshot and Settings.HitmarkerHeadshotSound) or (not isHeadshot and Settings.HitmarkerSound) then
            pcall(function()
                local sound = Instance.new("Sound")
                sound.SoundId = Settings.HitmarkerSoundId
                sound.Parent = workspace
                sound:Play()
                game:GetService("Debris"):AddItem(sound, 2)
            end)
        end

        if Settings.DamageIndicator.Enabled and damage and damage > 0 then
            local dmgText = getHitmarkerText()
            dmgText.Text = "-" .. damage
            dmgText.Color = Settings.DamageIndicator.Color
            dmgText.Size = Settings.DamageIndicator.Size
            dmgText.Position = Vector2.new(
                Camera.ViewportSize.X * Settings.DamageIndicator.Position.X,
                Camera.ViewportSize.Y * Settings.DamageIndicator.Position.Y
            )
            task.delay(Settings.DamageIndicator.Duration, function()
                dmgText.Visible = false
            end)
        end

        task.wait(2)
        marker.Visible = false
    end)
end

local hitRemotes = {}
local function findAllHitRemotes()
    local possibleNames = {
        "ProjectileInflict", "Hit", "Damage", "BulletHit", "InflictDamage",
        "HitPart", "HitMarker", "OnHit", "ProjectileHit", "DamageIndicator"
    }
    for _, name in ipairs(possibleNames) do
        local remote = ReplicatedStorage:FindFirstChild("Remotes") and ReplicatedStorage.Remotes:FindFirstChild(name)
        if remote and remote:IsA("RemoteEvent") then
            table.insert(hitRemotes, remote)
        end
    end
    for _, v in pairs(ReplicatedStorage:GetDescendants()) do
        if v:IsA("RemoteEvent") and not table.find(hitRemotes, v) then
            local nameLower = v.Name:lower()
            if nameLower:find("hit") or nameLower:find("inflict") or nameLower:find("damage") or nameLower:find("hurt") then
                table.insert(hitRemotes, v)
            end
        end
    end
    if #hitRemotes == 0 then
        warn("–•–∏—Ç–º–∞—Ä–∫–µ—Ä: –Ω–µ –Ω–∞–π–¥–µ–Ω–æ –Ω–∏ –æ–¥–Ω–æ–≥–æ remote-—Å–æ–±—ã—Ç–∏—è.")
    else
        print("–ù–∞–π–¥–µ–Ω–æ remote-—Å–æ–±—ã—Ç–∏–π –¥–ª—è –ø–æ–ø–∞–¥–∞–Ω–∏–π:", #hitRemotes)
    end
end

findAllHitRemotes()

for _, remote in ipairs(hitRemotes) do
    remote.OnClientEvent:Connect(function(...)
        local args = {...}
        local targetPlayer
        local hitPart
        local damage = 0

        for i, arg in ipairs(args) do
            if type(arg) == "userdata" and typeof(arg) == "Instance" and arg:IsA("Player") then
                targetPlayer = arg
            elseif type(arg) == "string" and (arg:find("Head") or arg:find("Torso") or arg:find("Leg")) then
                hitPart = arg
            elseif type(arg) == "number" and damage == 0 then
                damage = arg
            end
        end

        if targetPlayer then
            if targetPlayer == LocalPlayer then
                if Settings.HvH.AutoAntiAimOnHit.Enabled then
                    Settings.hitAntiAimUntil = tick() + Settings.HvH.AutoAntiAimOnHit.Duration
                end
            else
                local partName = tostring(hitPart):gsub("Enum.HitPart.", "")
                local isHead = partName == "Head" or (type(hitPart) == "string" and hitPart:lower():find("head"))
                local hitText = string.format("‚Üí %s (%s)", targetPlayer.Name, partName)
                addHitmarker(hitText, isHead, damage)

                if Settings.Combat.BulletTracers.Enabled then
                    local hitPos
                    if targetPlayer.Character then
                        local part = targetPlayer.Character:FindFirstChild("Head") or targetPlayer.Character:FindFirstChild("HumanoidRootPart")
                        if part then hitPos = part.Position end
                    end
                    if hitPos then
                        addTracer(Camera.CFrame.Position, hitPos)
                    end
                end
            end
        elseif hitPart then
            addHitmarker("‚Üí " .. tostring(hitPart), false, damage)
        else
            addHitmarker("‚Üí Hit!", false, damage)
        end
    end)
end

