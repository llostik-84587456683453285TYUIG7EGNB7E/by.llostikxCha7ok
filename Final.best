-- ‚ñà‚ñà‚ñà‚ñà‚ñà‚ñà‚ñà‚ñà‚ñà‚ñà‚ñà‚ñà‚ñà‚ñà‚ñà‚ñà‚ñà‚ñà‚ñà‚ñà‚ñà‚ñà‚ñà‚ñà‚ñà‚ñà‚ñà‚ñà‚ñà‚ñà‚ñà‚ñà‚ñà‚ñà‚ñà‚ñà‚ñà‚ñà‚ñà‚ñà‚ñà‚ñà‚ñà‚ñà‚ñà‚ñà‚ñà‚ñà‚ñà‚ñà‚ñà‚ñà‚ñà‚ñà‚ñà‚ñà‚ñà‚ñà‚ñà‚ñà
-- –ß–ê–°–¢–¨ 23: –û–°–ù–û–í–ù–û–ô –¶–ò–ö–õ (RenderStepped) - –ü–û–õ–ù–´–ô
-- ‚ñà‚ñà‚ñà‚ñà‚ñà‚ñà‚ñà‚ñà‚ñà‚ñà‚ñà‚ñà‚ñà‚ñà‚ñà‚ñà‚ñà‚ñà‚ñà‚ñà‚ñà‚ñà‚ñà‚ñà‚ñà‚ñà‚ñà‚ñà‚ñà‚ñà‚ñà‚ñà‚ñà‚ñà‚ñà‚ñà‚ñà‚ñà‚ñà‚ñà‚ñà‚ñà‚ñà‚ñà‚ñà‚ñà‚ñà‚ñà‚ñà‚ñà‚ñà‚ñà‚ñà‚ñà‚ñà‚ñà‚ñà‚ñà‚ñà‚ñà
local frameCount = 0
RunService.RenderStepped:Connect(function()
    frameCount = frameCount + 1
    local shouldUpdate = (frameCount % Settings.FRAME_SKIP == 0)

    if shouldUpdate then
        UpdateESP()
        UpdateAdminESP()
    end

    ApplyAntiAim()

    if Settings.Aimbot.ShowFOV and Settings.Aimbot.Enabled then
        local mousePos = UserInputService:GetMouseLocation()
        fovCircle.Position = mousePos
        fovCircle.Radius = Settings.Aimbot.FOV
        fovCircle.Color = Settings.Aimbot.FOVSettings.Color
        fovCircle.Thickness = Settings.Aimbot.FOVSettings.Thickness
        fovCircle.Transparency = Settings.Aimbot.FOVSettings.Transparency
        fovCircle.Visible = true
    else
        fovCircle.Visible = false
    end

    if Settings.holdingKey and Settings.Aimbot.Enabled then
        local target = GetClosestTarget()
        if target then
            local targetPos = target.Position
            local currentCF = Camera.CFrame
            local newCF = CFrame.lookAt(currentCF.Position, targetPos)
            Camera.CFrame = currentCF:Lerp(newCF, Settings.Aimbot.Smoothness)
        end
    end

    if Settings.isLagging and tick() >= Settings.lagEndTime then
        StopFakeLag()
    end

    -- Target Health
    if Settings.Combat.TargetHealth.Enabled then
        local aimTarget = GetClosestTarget()
        if aimTarget then
            local char = aimTarget.Parent
            local hum = char and char:FindFirstChildOfClass("Humanoid")
            if hum and hum.Health > 0 then
                local health = hum.Health
                if health ~= lastTargetHealth then
                    lastTargetHealth = health
                    targetHealthText.Text = string.format("HP: %d", health)
                end
                local viewportSize = Camera.ViewportSize
                local pos = (Settings.Combat.TargetHealth.Position == "BelowCrosshair")
                    and Vector2.new(viewportSize.X / 2, viewportSize.Y / 2 + 40)
                    or Vector2.new(20, 40)
                targetHealthText.Position = pos
                targetHealthText.Size = Settings.Combat.TargetHealth.FontSize
                targetHealthText.Color = Settings.Combat.TargetHealth.Color
                targetHealthText.Visible = true
            else
                targetHealthText.Visible = false
            end
        else
            targetHealthText.Visible = false
        end
    else
        targetHealthText.Visible = false
    end

    -- AntiSniper
    if Settings.Combat.AntiSniper.Enabled then
        local now = tick()
        if now - lastWarningTime > Settings.Combat.AntiSniper.Cooldown then
            local aimingPlayer = checkIfBeingAimed()
            if aimingPlayer then
                lastWarningTime = now
                local warningTextContent = Settings.Combat.AntiSniper.WarningText
                if Settings.Combat.AntiSniper.ShowName then
                    warningTextContent = warningTextContent .. "\n" .. aimingPlayer.Name
                end
                warningText.Text = warningTextContent
                local viewportSize = Camera.ViewportSize
                warningText.Position = Vector2.new(viewportSize.X / 2, viewportSize.Y / 2 - 100)
                warningText.Size = Settings.Combat.AntiSniper.WarningSize
                warningText.Color = Settings.Combat.AntiSniper.WarningColor
                warningText.Visible = true
                if Settings.Combat.AntiSniper.Sound then
                    pcall(function()
                        local sound = Instance.new("Sound")
                        sound.SoundId = Settings.HitmarkerSoundId
                        sound.Parent = workspace
                        sound:Play()
                        game:GetService("Debris"):AddItem(sound, 1)
                    end)
                end
                task.delay(1, function() warningText.Visible = false end)
            else
                warningText.Visible = false
            end
        end
    else
        warningText.Visible = false
    end

    -- Triggerbot
    if Settings.Triggerbot.Enabled then
        local key = keyMap[Settings.Triggerbot.Key]
        local isKeyDown = false
        if key then
            if key.EnumType == Enum.KeyCode then
                isKeyDown = UserInputService:IsKeyDown(key)
            else
                isKeyDown = UserInputService:IsMouseButtonPressed(key)
            end
        end

        if isKeyDown then
            local target = GetTargetUnderCrosshair()
            if target then
                Shoot()
            end
        end
    end

    if shouldUpdate then
        ApplyNoRecoilSpread()
    end

    if Settings.Visuals.NoFlash then
        for grenade in pairs(Settings.FlashGrenades) do
            if grenade and grenade.Parent then
                for _, child in ipairs(grenade:GetDescendants()) do
                    if child:IsA("ParticleEmitter") or child:IsA("Sound") then
                        child.Enabled = false
                    end
                end
            else
                Settings.FlashGrenades[grenade] = nil
            end
        end
    end

    if Settings.Visuals.NoVisor then
        for _, visor in ipairs(Settings.VisorParts) do
            if visor and visor.Parent then
                visor.LocalTransparencyModifier = 1
            end
        end
    end

    if frameCount % 1000 == 0 then
        local now = tick()
        for k, v in pairs(visibilityCache) do
            if now - v.timestamp > 10 then visibilityCache[k] = nil end
        end
        for k, v in pairs(Settings.VisibilityCache) do
            if now - v.timestamp > 10 then Settings.VisibilityCache[k] = nil end
        end
        for k, v in pairs(Settings.WeaponCache) do
            if now - v.timestamp > 10 then Settings.WeaponCache[k] = nil end
        end
    end

    local viewportSize = Camera.ViewportSize
    local fps = math.floor(1 / RunService.RenderStepped:Wait())
    watermark.Text = string.format("GGshop XENO ULTIMATE | FPS: %d", fps)
    watermark.Position = Vector2.new(viewportSize.X / 2, 40)
end)

