    state.Position = UDim2.new(0.6, 0, 0, 0)
    state.Font = Enum.Font.GothamBold
    state.TextSize = 16

    local function updateState()
        local val = settingTable[settingKey]
        if beta then
            state.Text = val and "BETA!!" or "false"
        else
            state.Text = tostring(val)
        end
        state.TextColor3 = val and COLORS.PINK_HOT or COLORS.INVISIBLE_RED
    end
    updateState()

    local btn = Instance.new("TextButton")
    btn.Parent = btnFrame
    btn.BackgroundTransparency = 1
    btn.Size = UDim2.new(1, 0, 1, 0)
    btn.Text = ""

    btn.MouseEnter:Connect(function()
        if desc and desc ~= "" then
            tooltip.Text = desc
            local mousePos = UserInputService:GetMouseLocation()
            tooltip.Position = Vector2.new(mousePos.X + 15, mousePos.Y + 15)
            tooltip.Visible = true
        end
    end)
    btn.MouseLeave:Connect(function()
        tooltip.Visible = false
    end)

    btn.MouseButton1Click:Connect(function()
        settingTable[settingKey] = not settingTable[settingKey]
        updateState()
        if settingTable == Settings.Misc and settingKey == "NightVision" then
            ApplyNightVision(Settings.Misc.NightVision)
        end
        if settingTable == Settings.Misc and settingKey == "FPSBoost" then
            ApplyFPSBoost(Settings.Misc.FPSBoost)
        end
    end)
