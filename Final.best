    slider.Position = UDim2.new(0, 15, 0, 35)

    local fill = Instance.new("Frame")
    fill.Parent = slider
    fill.BackgroundColor3 = COLORS.PINK_HOT
    fill.Size = UDim2.new((settingTable[settingKey] - min) / (max - min), 0, 1, 0)
    fill.BorderSizePixel = 0

    local btn = Instance.new("TextButton")
    btn.Parent = sFrame
    btn.BackgroundTransparency = 1
    btn.Size = UDim2.new(1, 0, 1, 0)
    btn.Text = ""

    local desc = displayName .. " (" .. min .. " - " .. max .. ")"
    btn.MouseEnter:Connect(function()
        tooltip.Text = desc
        local mousePos = UserInputService:GetMouseLocation()
        tooltip.Position = Vector2.new(mousePos.X + 15, mousePos.Y + 15)
        tooltip.Visible = true
    end)
    btn.MouseLeave:Connect(function()
        tooltip.Visible = false
    end)

    local dragging = false
    btn.MouseButton1Down:Connect(function() dragging = true end)
    UserInputService.InputEnded:Connect(function(input)
        if input.UserInputType == Enum.UserInputType.MouseButton1 then dragging = false end
    end)
    btn.MouseMoved:Connect(function()
        if dragging then
            local mousePos = UserInputService:GetMouseLocation()
            local absPos = slider.AbsolutePosition
            local relX = math.clamp(mousePos.X - absPos.X, 0, slider.AbsoluteSize.X)
            local value = min + (relX / slider.AbsoluteSize.X) * (max - min)
            value = math.floor(value * 10) / 10
            settingTable[settingKey] = value
            fill.Size = UDim2.new((value - min) / (max - min), 0, 1, 0)
            label.Text = displayName .. ": " .. tostring(value) .. (format or "")
        end
    end)
end

